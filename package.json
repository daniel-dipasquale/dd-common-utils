{
  "author": "Daniel Di Pasquale",
  "bugs": {
    "url": "https://github.com/daniel-dipasquale/dd-common-utils/issues"
  },
  "dependencies": {
    "jshint": "^2.11.1"
  },
  "description": "common utils",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "proxyquire": "^2.1.3"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/daniel-dipasquale/dd-common-utils",
  "jshintConfig": {
    "boss": true,
    "eqeqeq": true,
    "eqnull": true,
    "esversion": 9,
    "expr": true,
    "laxbreak": true,
    "mocha": true,
    "node": true,
    "quotmark": "single",
    "undef": true,
    "unused": true
  },
  "keywords": [
    "async",
    "common",
    "generator",
    "iterable",
    "iterator",
    "promise",
    "utils"
  ],
  "license": "ISC",
  "main": "index.js",
  "name": "@daniel-dipasquale/dd-common-utils",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/daniel-dipasquale/dd-common-utils.git"
  },
  "scripts": {
    "test": "mocha 'lib/{,!(node_modules)/**}/*-test.js' --recursive && jshint lib",
    "test-single": "mocha $1",
    "test-with-coverage": "nyc --reporter=text mocha 'lib/{,!(node_modules)/**}/*-test.js' --recursive && jshint lib"
  },
  "version": "1.0.9"
}
